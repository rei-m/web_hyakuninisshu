    "style-loader": "^0.19.1",
    "ts-jest": "^22.0.1",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.0.0",
    "typed-css-modules": "^0.3.1",
    "typed-css-modules-loader": "^0.0.11",
