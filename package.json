{
  "name": "hyakuninisshu",
  "version": "1.0.0",
  "description": "training of HyakunInisshu",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --progress --colors --config webpack.dev.js",
    "test": "jest",
    "test-update": "jest --updateSnapshot",
    "test-cov": "node --max_old_space_size=2048 $(npm bin)/jest --maxWorkers=2 --coverage",
    "lint": "tslint -c tslint.json '{src,__tests__}/**/*.{ts,tsx}'",
    "lint-fix": "tslint --fix -c tslint.json '{src,__tests__}/**/*.{ts,tsx}'",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rei-m/web_hyakuninisshu.git"
  },
  "author": "rei-m",
  "license": "MIT",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/node_modules/jest-enzyme/lib/index.js",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileTransformer.js"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/factories/",
      "/__tests__/helpers/"
    ],
    "setupFiles": [
      "./jest.setup.ts"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.ts?(x)"
    ],
    "coverageDirectory": "./coverage/",
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-redux",
      "enzyme",
      "jest-enzyme",
      "fbjs"
    ],
    "moduleNameMapper": {
      "@src/(.*)": "<rootDir>/src/$1"
    }
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/react-helmet": "^5.0.5",
    "@types/react-redux": "^5.0.15",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-test-renderer": "^16.0.1",
    "@types/react-transition-group": "^2.0.8",
    "@types/recompose": "^0.26.0",
    "@types/redux-mock-store": "^0.0.13",
    "codecov": "^3.0.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "es6-promise": "^4.2.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "jest-enzyme": "^6.0.0",
    "prettier": "1.12.1",
    "react-test-renderer": "^16.3.2",
    "redux-mock-store": "~1.4.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.21.0",
    "ts-jest": "^22.4.4",
    "ts-loader": "^4.2.0",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "tslint-loader": "^3.6.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.5.1",
    "typed-css-modules": "^0.3.2",
    "typed-css-modules-loader": "^0.0.12",
    "typescript": "^2.8.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "@blueprintjs/core": "^2.2.1",
    "@blueprintjs/icons": "^2.1.1",
    "axios": "^0.18.0",
    "formik": "^0.11.11",
    "normalize.css": "^8.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-ga": "^2.5.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.3.1",
    "recompose": "^0.27.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.2.6"
  }
}
