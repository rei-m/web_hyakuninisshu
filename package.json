{
  "name": "hyakuninisshu",
  "version": "2.0.0",
  "description": "training of HyakunInisshu",
  "author": "rei-m",
  "dependencies": {
    "@material-ui/core": "^3.8.1",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "formik": "^1.4.1",
    "gatsby": "^2.0.84",
    "gatsby-image": "^2.0.20",
    "gatsby-plugin-google-analytics": "^2.0.8",
    "gatsby-plugin-manifest": "^2.0.13",
    "gatsby-plugin-no-sourcemaps": "^2.0.1",
    "gatsby-plugin-offline": "^2.0.16",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-remove-trailing-slashes": "^2.0.6",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-plugin-styled-components": "^3.0.4",
    "gatsby-plugin-typescript": "^2.0.3",
    "gatsby-plugin-typography": "^2.2.4",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-sharp": "^2.1.10",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.0",
    "react-typography": "^0.16.18",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.3",
    "typography": "^0.16.18"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "test-update": "jest --updateSnapshot",
    "test-cov": "node --max_old_space_size=2048 $(npm bin)/jest --maxWorkers=2 --coverage",
    "lint": "tslint -c tslint.json '{src,__tests__}/**/*.{ts,tsx}'",
    "lint-fix": "tslint --fix -c tslint.json '{src,__tests__}/**/*.{ts,tsx}'",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/graphql": "^14.0.4",
    "@types/react-helmet": "^5.0.8",
    "@types/react-redux": "^6.0.12",
    "@types/recompose": "^0.30.2",
    "@types/styled-components": "^4.1.4",
    "@types/typography": "^0.16.1",
    "babel-plugin-styled-components": "^1.10.0",
    "prettier": "^1.15.2",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rei-m/web_hyakuninisshu.git"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/node_modules/jest-enzyme/lib/index.js",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileTransformer.js"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/factories/",
      "/__tests__/helpers/"
    ],
    "setupFiles": [
      "./jest.setup.ts"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.ts?(x)"
    ],
    "coverageDirectory": "./coverage/",
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-redux",
      "enzyme",
      "jest-enzyme",
      "fbjs"
    ],
    "moduleNameMapper": {
      "@src/(.*)": "<rootDir>/src/$1"
    }
  }
}
