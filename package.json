{
  "name": "hyakuninisshu",
  "version": "1.0.0",
  "description": "training of HyakunInisshu",
  "main": "index.js",
  "scripts": {
    "build": "webpack --progress --colors",
    "start": "webpack-dev-server --progress --colors",
    "test": "jest",
    "update-snapshot": "jest --updateSnapshot",
    "lint": "tslint -c tslint.json '{src,__tests__}/**/*.{ts,tsx}'",
    "lint-fix": "tslint --fix -c tslint.json '{src,__tests__}/**/*.{ts,tsx}'",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rei-m/web_hyakuninisshu.git"
  },
  "author": "rei-m",
  "license": "MIT",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/node_modules/jest-enzyme/lib/index.js",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileTransformer.js"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/factories/",
      "/__tests__/helpers/"
    ],
    "setupFiles": [
      "./jest.setup.ts"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.ts?(x)"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react",
      "node_modules/react-dom",
      "node_modules/react-redux",
      "node_modules/enzyme",
      "node_modules/jest-enzyme",
      "node_modules/fbjs"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jest": "^22.1.4",
    "@types/nock": "^9.1.2",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "@types/react-helmet": "^5.0.5",
    "@types/react-redux": "^5.0.15",
    "@types/react-router-dom": "^4.2.4",
    "@types/react-test-renderer": "^16.0.1",
    "@types/react-transition-group": "^2.0.7",
    "@types/recompose": "^0.24.4",
    "@types/redux-mock-store": "^0.0.13",
    "awesome-typescript-loader": "^3.5.0",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "es6-promise": "^4.2.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.4",
    "jest": "^22.4.2",
    "jest-enzyme": "^4.2.0",
    "nock": "^9.2.3",
    "prettier": "1.11.1",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "~1.4.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.20.2",
    "ts-jest": "^22.4.1",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.9.0",
    "tslint-loader": "^3.6.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.5.1",
    "typed-css-modules": "^0.3.1",
    "typed-css-modules-loader": "^0.0.11",
    "typescript": "^2.7.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^2.0.0-rc.2",
    "@blueprintjs/icons": "^2.0.0-rc.2",
    "formik": "^0.11.11",
    "isomorphic-fetch": "^2.2.1",
    "normalize.css": "^8.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.1",
    "recompose": "^0.26.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.2.0"
  }
}
